{
    "name": "eyewitness/eye4",
    "description": "Eyewitness.io client for Laravel 4.2 applications",
    "keywords": [
        "laravel 4.2",
        "queue",
        "cron",
        "monitor",
        "eyewitness"
    ],
    "homepage": "https://github.com/eyewitness/eye4",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurence",
            "email": "laurence@eyewitness.io"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "guzzlehttp/guzzle": "4.*|5.*|6.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "psr-0": {
            "Eyewitness\\Eye\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eyewitness\\Eye\\Test\\": "tests"
        },
        "classmap": ["tests/TestCase.php"]
    },
    "minimum-stability": "stable"
}
